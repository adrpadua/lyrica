<?xml version='1.0' encoding='utf-8'?>
<widget id="edu.uci.hack.adrian.brent" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Lyrica</name>
    <description>
        Hype up parties and be a freestyle legend! Lyrica app gives you rhymes and a beat so you can get to rappin'. Tame your skills, and overtime, become a freestyle legend. 
    </description>
    <author email="badixon@uci.edu;arpadua@uci.edu;mrutroug@uci.edu" href="https://Linkedin.com">
        The Pollies'
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="orientation" value="portrait" />
</widget>
